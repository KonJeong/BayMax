<br><font size=5, color="#662500"><b>ID 찾기</b></font><br><br><br>
<br><br><br>
<div><h4><%= flash[:alert] %></h4></div>
<%= form_for :search, url: {action: "id_search_result"} do |f| %>
<table width="100%" border="0" cellspacing="0" cellpadding="0">
	<tr>
		<td align="center"><font size=5> Email 확인 </font></td>
		<td><%= f.email_field :Email, size:25, maxlength:25, required:true %> &nbsp;&nbsp;<%= f.submit '찾기' %></td>
	</tr>
</table>
<% end %>
